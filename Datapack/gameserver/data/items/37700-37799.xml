<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://l2j.org" xsi:schemaLocation="http://l2j.org items.xsd">
	<weapon id="37700" name="Valentine Dual Dagger" type="DUAL_DAGGER" body-part="TWO_HAND" icon="icon.ev_hallowinday_candy_i00">
	<!-- Modifies the appearance of a weapon to that of a Valentine Dual Dagger. -->
		<restriction freightable="false" tradable="false" dropable="false" sellable="false" private-sellable="false"/>
		<attributes commission-type="OTHER_ITEM" enchant-enabled="false"/>
		<crystal />
		<damage radius="40" angle="240"/>
		<stats>
			<stat type="ACCURACY" value="-3.75"/>
			<stat type="CRITICAL_RATE" value="12.0"/>
			<stat type="MAGIC_ATTACK" value="414.0"/>
			<stat type="PHYSICAL_ATTACK" value="752.0"/>
			<stat type="PHYSICAL_ATTACK_RANGE" value="40.0"/>
			<stat type="PHYSICAL_ATTACK_SPEED" value="433.0"/>
			<stat type="RANDOM_DAMAGE" value="10.0"/>
		</stats>
	</weapon>

	<weapon id="37701" name="Valentine Warrior Dual Blunt Weapon" type="DUAL_BLUNT" body-part="TWO_HAND" icon="icon.ev_hallowinday_candy_i00">
	<!-- Modifies the appearance of a weapon to that of a Valentine Dual Blunt Weapon. -->
		<restriction freightable="false" tradable="false" dropable="false" sellable="false" private-sellable="false"/>
		<attributes commission-type="OTHER_ITEM" enchant-enabled="false"/>
		<crystal />
		<damage radius="40" angle="240"/>
		<stats>
			<stat type="ACCURACY" value="8.0"/>
			<stat type="CRITICAL_RATE" value="6.0"/>
			<stat type="MAGIC_ATTACK" value="414.0"/>
			<stat type="PHYSICAL_ATTACK" value="916.0"/>
			<stat type="PHYSICAL_ATTACK_RANGE" value="40.0"/>
			<stat type="PHYSICAL_ATTACK_SPEED" value="325.0"/>
			<stat type="RANDOM_DAMAGE" value="20.0"/>
		</stats>
	</weapon>

	<armor id="37702" name="Valentine Shield" type="SHIELD" body-part="LEFT_HAND" icon="icon.ev_hallowinday_candy_i00">
	<!-- Modifies the appearance of a weapon to that of a Valentine Shield. -->
		<restriction freightable="false" tradable="false" dropable="false" sellable="false" private-sellable="false"/>
		<attributes enchant-enabled="false" />
		<crystal />
		<stats>
			<stat type="EVASION_RATE" value="-8.0"/>
			<stat type="SHIELD_DEFENCE" value="546.0"/>
			<stat type="SHIELD_DEFENCE_RATE" value="20.0"/>
		</stats>
	</armor>

	<armor id="37703" name="Valentine Sigil" type="SIGIL" body-part="LEFT_HAND" icon="icon.ev_hallowinday_candy_i00">
	<!-- Modifies the appearance of a sigil to that of a Valentine Sigil. -->
		<restriction freightable="false" tradable="false" dropable="false" sellable="false" private-sellable="false"/>
		<attributes enchant-enabled="false" />
		<crystal />
		<stats>
			<stat type="PHYSICAL_ATTACK_RANGE" value="40.0"/>
			<stat type="PHYSICAL_DEFENCE" value="150.0"/>
		</stats>
	</armor>

	<item id="37704" name="Love Potion" type="NONE" icon="icon.etc_whiteday_herb">
	<!-- Herb to change the appearance of a Valentine's Day weapon. -->
		<restriction depositable="false" clan-depositable="false" castle-depositable="false" freightable="false"/>
		<attributes for-npc="true" immediate-effect="true" ex-immediate-effect="true"/>
		<skill-reducer>
			<skill id="17154" value="-1"/>  <!-- Love Potion -->
		</skill-reducer>
	</item>

	<item id="37705" name="Gold Fishing Shot" type="SOULSHOT" icon="icon.etc_spirit_bullet_fish_i05">
	<!-- Fishing consumables. Fishing success +100%. Cannot be sold in the grocery store. -->
		<restriction stackable="true" sellable="false"/>
		<attributes weight="3" price="56" immediate-effect="true" action="FISHINGSHOT"/>
	</item>

	<armor id="37725" name="Dinos Uniform" type="HEAVY" body-part="ALL_DRESS" icon="icon.london_olympic_bikini">
	<!-- Dinos Uniform. Cannot be exchanged, dropped, or sold. Can be stored in a private warehouse. -->
		<restriction freightable="false" tradable="false" dropable="false" sellable="false" private-sellable="false"/>
		<attributes weight="1000" enchant-enabled="false" />
		<crystal />
	</armor>

	<armor id="37726" name="Dinos Shortstop Cap" type="NONE" body-part="HAIR_ALL" icon="icon.accessory_mining_cap_i00">
	<!-- Dinos Defense Hat. -->
		<restriction freightable="false" destroyable="false" tradable="false" dropable="false" sellable="false" private-sellable="false"/>
		<attributes enchant-enabled="false" />
		<crystal />
	</armor>

</list>