	<item id="6035" name="Magic Haste Potion" type="POTION" icon="icon.etc_potion_of_acumen2_i00">
	<!-- Casting Spd. is raised. -->
		<restriction olympiad-restricted="true" stackable="true"/>
		<attributes weight="20" price="2400" commission-type="POTION" reuse-delay="500" for-npc="true" immediate-effect="true" action="SKILL_REDUCE" auto-use="SUPPLY"/>
